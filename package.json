    "@react-navigation/native-stack": "^7.3.16",
    "@stripe/stripe-react-native": "^0.57.2",
    "@tanstack/react-query": "^5.90.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^12.5.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "expo": "^54.0.23",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.12",
    "expo-document-picker": "^14.0.8",
